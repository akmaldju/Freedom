{
  "name": "css-freedom",
  "version": "0.2.6",
  "description": "CSS FreeDOM - a light-weight simplified and improved version of Bootstrap.",
  "main": "index.js",
  "devDependencies": {
    "sass": "1.86.3"
  },
  "scripts": {
    "compile": "sass scss/freedom.scss dist/freedom.css --style=expanded && sass scss/freedom.scss dist/freedom.min.css --style=compressed",
    "minify": "sass scss/freedom.scss dist/freedom.min.css --style=compressed",
    "publish": "pnpm run compile && pnpm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "sass --watch scss:dist --style=expanded"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akmaldju/Freedom.git"
  },
  "keywords": [
    "freedom",
    "css",
    "scss",
    "classes"
  ],
  "author": "Akmal Djumakhodjaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akmaldju/Freedom/issues"
  },
  "homepage": "https://github.com/akmaldju/Freedom#readme"
}
